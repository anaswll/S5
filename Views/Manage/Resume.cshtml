@model ResumeViewModel
@{
    ViewData["Title"] = "About me";
    ViewData.AddActivePage(ManageNavPages.Resume);
}

<h4>@ViewData["Title"]</h4>

<div class="row">
    <div class="col-md-6">
        <form method="post">
            <div asp-validation-summary="All" class="text-danger"></div>

            <div class="form-group">
                <label asp-for="fname">First name</label>
                <input asp-for="fname" class="form-control" disabled />
            </div>

            <div class="form-group">
                <label asp-for="lname">Last name</label>
                <input asp-for="lname" class="form-control" disabled />
            </div>
            <div class="form-group">
                <label asp-for="Label">Label</label>
                <input asp-for="Label" class="form-control" disabled />
            </div>
            <div class="form-group">
                <label asp-for="Age">Age</label>
                <input asp-for="Age" class="form-control" disabled />
            </div>
            <div class="form-group">
                <label asp-for="Phone">Phone</label>
                <input asp-for="Phone" class="form-control" disabled />
            </div>
            <div class="form-group">
                <label asp-for="Address">Address</label>
                <input asp-for="Address" class="form-control" disabled />
            </div>
            <div class="form-group">
                <label asp-for="City">City</label>
                <input asp-for="City" class="form-control" disabled />
            </div>
            <div class="form-group">
                <label asp-for="Country">Country</label>
                <input asp-for="Country" class="form-control" disabled />
            </div>


            <button type="submit" class="btn btn-default">Save</button>
        </form>
    </div>
</div>

@section Scripts {
    @await Html.PartialAsync("_ValidationScriptsPartial")
}